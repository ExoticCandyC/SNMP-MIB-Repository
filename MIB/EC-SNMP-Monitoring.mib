    -- 1.3.6.1.4.1.59097.1.SNMP-Monitoring(1).SNMP-Manager(2)
    Trap-Redirection-Target     OBJECT IDENTIFIER ::= { SNMP-Manager       1 }

    -- 1.3.6.1.4.1.59097.1.1.SNMP-Manager(2).Trap-Redirection-Target(1)
    IP OBJECT-TYPE
        SYNTAX  IpAddress
        ACCESS  read-write
        STATUS  current
        DESCRIPTION
            "The IP address of the SNMP manager who is supposed to receive
             forwarded traps from the agents being managed by this devide."
        ::= { Trap-Redirection-Target 1 }

    -- 1.3.6.1.4.1.59097.1.1.SNMP-Manager(2).Trap-Redirection-Target(1)
    Port OBJECT-TYPE
        SYNTAX  PortNumber
        ACCESS  read-write
        STATUS  current
        DEFVAL  { 162 }
        DESCRIPTION
            "The port number of the SNMP manager who is supposed to receive
             forwarded traps from the agents being managed by this devide."
        ::= { Trap-Redirection-Target 2 }

    -- 1.3.6.1.4.1.59097.1.1.SNMP-Manager(2)
    Query-Interval OBJECT-TYPE
        SYNTAX  Unsigned32
        ACCESS  read-write
        STATUS  current
        DEFVAL  { 500 }
        DESCRIPTION
            "The interval between each SNMP get query in milliseconds.
             Note that lower interval directly increases the network
             load and can cause problems. Greater interval can cause
             slower refresh rate for the agents' data."
        ::= { SNMP-Manager 2 }

    -- 1.3.6.1.4.1.59097.1.1.SNMP-Manager(2)
    Agents-Count OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  current
        DESCRIPTION
            "The total number of SNMP Agents the device can manage."
        ::= { SNMP-Manager 3 }

    -- 1.3.6.1.4.1.59097.1.1.SNMP-Manager(2)
    Agents-Table OBJECT-TYPE
        SYNTAX  SEQUENCE OF Agent-Entry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
            "A list of all the SNMP agents managed by the device and
             all the informations about them."
        ::= { SNMP-Manager 4 }

    -- 1.3.6.1.4.1.59097.1.1.SNMP-Manager(2).Agents-Table(4)
    Agent-Entry OBJECT-TYPE
        SYNTAX  Agent-Entry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
            "An entry containing all the information about the
             SNMP agenets managed by the device."
        INDEX
        {
            Agent-Index
        }
        ::= { Agents-Table 1 }

    Agent-Entry ::=
        SEQUENCE {
            Agent-Index         INTEGER,
            Agent-IP            IpAddress,
            Agent-Port          PortNumber
        }

    -- 1.3.6.1.4.1.59097.1.1.SNMP-Manager(2).Agents-Table(4).Agent-Entry(1)
    Agent-Index OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  current
        DESCRIPTION
            "A unique value for each SNMP agent. Its value
            ranges between 1 and the value of Agents-Count."
        ::= { Agent-Entry 1 }

    -- 1.3.6.1.4.1.59097.1.1.SNMP-Manager(2).Agents-Table(4).Agent-Entry(1)
    Agent-IP OBJECT-TYPE
        SYNTAX  IpAddress
        ACCESS  read-write
        STATUS  current
        DESCRIPTION
            "The IP address of the agent by which the manager can
             read the agent."
        ::= { Agent-Entry 2 }

    -- 1.3.6.1.4.1.59097.1.1.SNMP-Manager(2).Agents-Table(4).Agent-Entry(1)
    Agent-Port OBJECT-TYPE
        SYNTAX  PortNumber
        ACCESS  read-write
        STATUS  current
        DEFVAL  { 161 }
        DESCRIPTION
            "The port number of the agent by which the manager can
             read the agent."
        ::= { Agent-Entry 3 }

    -- 1.3.6.1.4.1.59097.1.1.SNMP-Manager(2).Agents-Table(4).Agent-Entry(1)
    Agent-Name OBJECT-TYPE
        SYNTAX  OCTET STRING (SIZE (4..30))
        ACCESS  read-write
        STATUS  current
        DESCRIPTION
            "A textual string containing the chosen name of the
             target."
        ::= { Agent-Entry 4 }

    -- 1.3.6.1.4.1.59097.1.1.SNMP-Manager(2).Agents-Table(4).Agent-Entry(1)
    Agent-Community-String OBJECT-TYPE
        SYNTAX  OCTET STRING (SIZE (0..50))
        ACCESS  read-write
        STATUS  current
        DEFVAL  { "public" }
        DESCRIPTION
            "The community string used to read values from the agent."
        ::= { Agent-Entry 5 }

    -- 1.3.6.1.4.1.59097.1.1.SNMP-Manager(2).Agents-Table(4).Agent-Entry(1)
    Agent-OID OBJECT-TYPE
        SYNTAX  OBJECT IDENTIFIER
        ACCESS  read-write
        STATUS  current
        DESCRIPTION
            "The OID of the target value that should be read from the agent."
        ::= { Agent-Entry 6 }

    -- 1.3.6.1.4.1.59097.1.1.SNMP-Manager(2).Agents-Table(4).Agent-Entry(1)
    Agent-State OBJECT-TYPE
        SYNTAX  INTEGER
        {
            Unreachable(0),
            Active(1)
        }
        ACCESS  read-only
        STATUS  current
        DESCRIPTION
            "The status of the target agent."
        ::= { Agent-Entry 7 }

    -- 1.3.6.1.4.1.59097.1.1.SNMP-Manager(2).Agents-Table(4).Agent-Entry(1)
    Agent-Value OBJECT-TYPE
        SYNTAX  OCTET STRING (SIZE (0..30))
        ACCESS  read-only
        STATUS  current
        DESCRIPTION
            "The value read from the agent, represented as the string form."
        ::= { Agent-Entry 8 }

