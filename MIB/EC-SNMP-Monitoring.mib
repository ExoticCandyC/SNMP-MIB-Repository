        ::= { Energy-Analyzer 1 }

    -- 1.3.6.1.4.1.59097.1.1.Energy-Analyzer(3)
    Energy-Analyzer-State OBJECT-TYPE
        SYNTAX  INTEGER
        {
            Unreachable(0),
            Active(1)
        }
        ACCESS  read-only
        STATUS  current
        DESCRIPTION
            "The status of the energy analyzer."
       ::= { Energy-Analyzer 2 }

    -- 1.3.6.1.4.1.59097.1.1.Energy-Analyzer(3)
    Parameters-Count OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  current
        DESCRIPTION
            "The total number of the target values the device will query
             from the energy analyzer."
        ::= { Energy-Analyzer 3 }

    -- 1.3.6.1.4.1.59097.1.1.Energy-Analyzer(3)
    Parameters-Table OBJECT-TYPE
        SYNTAX  SEQUENCE OF Parameter-Entry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
            "A list of all the parameters read from the energy analyzer
             and all the informations about them."
        ::= { Energy-Analyzer 4 }

    -- 1.3.6.1.4.1.59097.1.1.Energy-Analyzer(3).Parameters-Table(4)
    Parameter-Entry OBJECT-TYPE
        SYNTAX  Parameter-Entry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
            "An entry containing all the information about the
             SNMP agenets managed by the device."
        INDEX
        {
            Parameter-Index
        }
        ::= { Parameters-Table 1 }

    Parameter-Entry ::=
        SEQUENCE {
            Parameter-Index             INTEGER,
            Parameter-MODBUS-Register   INTEGER,
            Parameter-Name              DisplayString,
            Parameter-Value             FLOAT,
            Parameter-Unit-Name         DisplayString
        }

    -- 1.3.6.1.4.1.59097.1.1.Energy-Analyzer(3).Parameters-Table(4)
    Parameter-Index OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  current
        DESCRIPTION
            "A unique value for energy analyzer parameter. Its value
            ranges between 1 and the value of Parameters-Count."
        ::= { Parameter-Entry 1 }

    -- 1.3.6.1.4.1.59097.1.1.Energy-Analyzer(3).Parameters-Table(4)
    Parameter-MODBUS-Register OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  current
        DESCRIPTION
            "The MODBUS register of the value. It can be found on the
             energy analyzer's datasheet."
        ::= { Parameter-Entry 2 }

    -- 1.3.6.1.4.1.59097.1.1.Energy-Analyzer(3).Parameters-Table(4)
    Parameter-Name OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-only
        STATUS  current
        DESCRIPTION
            "The name of the value."
        ::= { Parameter-Entry 3 }

    -- 1.3.6.1.4.1.59097.1.1.Energy-Analyzer(3).Parameters-Table(4)
    Parameter-Value OBJECT-TYPE
        SYNTAX  FLOAT
        ACCESS  read-only
        STATUS  current
        DESCRIPTION
            "The value read from the energy analyzer."
        ::= { Parameter-Entry 4 }

    -- 1.3.6.1.4.1.59097.1.1.Energy-Analyzer(3).Parameters-Table(4)
    Parameter-Unit-Name OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-only
        STATUS  current
        DESCRIPTION
            "The unit name of the given value. For example 'V' for voltage."
        ::= { Parameter-Entry 5 }

