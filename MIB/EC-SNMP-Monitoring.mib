    -- 1.3.6.1.4.1.59097.1.SNMP-Monitoring(1).System-Status(1).RTCC(2)
    RTCC-Categorized            OBJECT IDENTIFIER ::= { RTCC               2 }

    -- 1.3.6.1.4.1.59097.1.SNMP-Monitoring(1).System-Status(1).RTCC(2)
    Clock-Calendar OBJECT-TYPE
        SYNTAX  TIME
        ACCESS  read-write
        STATUS  current
        DESCRIPTION
            "The combination of date, time and time zone as understood
             by the device's realtime clock and calendar."
        ::= { RTCC 1 }

    -- 1.3.6.1.4.1.59097.1.1.System-Status(1).RTCC(2).RTCC-Categorized(2)
    Clock OBJECT-TYPE
        SYNTAX  TIME-OF-DAY
        ACCESS  read-write
        STATUS  current
        DESCRIPTION
            "The time of day as understood by the device's internal clock.
             Please not that this object is a subset of RTCC object."
        ::= { RTCC-Categorized 1 }

    -- 1.3.6.1.4.1.59097.1.1.System-Status(1).RTCC(2).RTCC-Categorized(2)
    Calendar OBJECT-TYPE
        SYNTAX  DATE
        ACCESS  read-write
        STATUS  current
        DESCRIPTION
            "The date of year as understood by the device's internal calendar.
             Please not that this object is a subset of RTCC object."
        ::= { RTCC-Categorized 2 }

    -- 1.3.6.1.4.1.59097.1.1.System-Status(1).RTCC(2).RTCC-Categorized(2)
    Time-Zone OBJECT-TYPE
        SYNTAX  OCTET STRING (SIZE (6))
        ACCESS  read-write
        STATUS  current
        DESCRIPTION
            "The time-zone set for the device's internal clock.
             The format is +HH:MM or -HH:MM
             where      HH   - time zone hours offset (-13 through +13)
                        MM   - time zone minutes offset (00 or 30)
             Note that both numbers must be padded with 0.
             For example UTC is represented as +00:00"
        ::= { RTCC-Categorized 3 }

