    -- 1.3.6.1.4.1.59097.1.SNMP-Monitoring(1).Traps-Settings(7)
    Maximum-Trap-Level OBJECT-TYPE
        SYNTAX  INTEGER
        {
            emergency     (0),      --system is unusable
            alert         (1),      --action must be taken immediately
            critical      (2),      --critical conditions
            error         (3),      --error conditions
            warning       (4),      --warning conditions
            notice        (5),      --normal but significant condition
            informational (6),      --informational messages
            debug         (7)       --debug-level messages
        }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "This objects dictates which traps are supposed to be generated.
             For example if this object is set to 'critical(2)', only the top
             3 severity traps will be sent to the trap receiver.
             top 3 severity traps are:
                 emergency     (0)
                 alert         (1)
                 critical      (2)"
        ::= { Traps-Settings 4 }


    -- 1.3.6.1.4.1.59097.1.SNMP-Monitoring(1).Traps-Settings(7)
    Persistent-Trap-Interval OBJECT-TYPE
        SYNTAX  INTEGER (1..60)
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "This objects defines the interval, in minutes, between the
             generation of each specific trap. For example if the SHT20
             module is not working, and this object is set to 5, the
             SHT20-Failure trap will be produced every 5 minutes.
             Please note that this setting ONLY applies to the persistent
             trap conditions and doesn't affect one-time traps like the
             Login-Trap."
        ::= { Traps-Settings 5 }


