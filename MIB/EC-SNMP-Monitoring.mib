    -- 1.3.6.1.4.1.59097.1.SNMP-Monitoring(1).System-Status(1).Actuators(3)
    actuatorCount OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "The total number of actuators the device has."
        ::= { Actuators 1 }

    -- 1.3.6.1.4.1.59097.1.SNMP-Monitoring(1).System-Status(1).Actuators(3)
    actuatorTable OBJECT-TYPE
        SYNTAX  SEQUENCE OF actuatorEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
            "A list of all the actuators controlled by the device and
             all the informations about them."
        ::= { Actuators 2 }

    -- 1.3.6.1.4.1.59097.1.1.System-Status(1).Actuators(3).actuatorTable(2)
    actuatorEntry OBJECT-TYPE
        SYNTAX  actuatorEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
            "An actuator entry containing all the information about the
             actuator status."
        INDEX
        {
            actuatorIndex
        }
        ::= { actuatorTable 1 }

    actuatorEntry ::=
        SEQUENCE {
            actuatorIndex       INTEGER,
            actuatorType        INTEGER,
            actuatorName        DisplayString,
            actuatorValue       INTEGER,
            actuatorStatus      INTEGER
        }

    -- 1.3.6.1.4.1.59097.1.1.1.Actuators(3).Actuators(3).actuatorEntry(1)
    actuatorIndex OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  current
        DESCRIPTION
            "A unique value for each actuator. Its value
            ranges between 1 and the value of actuatorCount."
        ::= { actuatorEntry 1 }

    -- 1.3.6.1.4.1.59097.1.1.1.Actuators(3).Actuators(3).actuatorEntry(1)
    actuatorType OBJECT-TYPE
        SYNTAX  INTEGER
        {
            Relay(1)
        }
        ACCESS  read-only
        STATUS  current
        DESCRIPTION
            "The type of the actuator."
        ::= { actuatorEntry 2 }

    -- 1.3.6.1.4.1.59097.1.1.1.Actuators(3).Actuators(3).actuatorEntry(1)
    actuatorName OBJECT-TYPE
        SYNTAX  DisplayString (SIZE (4..20))
        ACCESS  read-write
        STATUS  current
        DESCRIPTION
            "A textual string containing the chosen name of the
             actuator."
        ::= { actuatorEntry 3 }

    -- 1.3.6.1.4.1.59097.1.1.1.Actuators(3).Actuators(3).actuatorEntry(1)
    actuatorValue OBJECT-TYPE
        SYNTAX  INTEGER
        {
            Deactive(0),
            Active(1)
        }
        ACCESS  read-write
        STATUS  current
        DESCRIPTION
            "The value of the actuator."
        ::= { actuatorEntry 4 }

    -- 1.3.6.1.4.1.59097.1.1.1.Actuators(3).Actuators(3).actuatorEntry(1)
    actuatorStatus OBJECT-TYPE
        SYNTAX  INTEGER
        {
            Manual(0),
            Automatic(1)
        }
        ACCESS  read-write
        STATUS  current
        DESCRIPTION
            "The status of the actuator. This variable dictates if the
             actuator follows the predefined algorithms or it is being
             controlled manually."
        ::= { actuatorEntry 5 }

